{
  "name": "express-socketio-redis-boilerplate",
  "version": "1.0.0",
  "description": "express-socketio-redis-boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "node_modules/.bin/cross-env NODE_ENV=production node ./build/app.js",
