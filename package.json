{
  "name": "express-socketio-redis-boilerplate",
  "version": "1.0.0",
  "description": "express-socketio-redis-boilerplate",
  "main": "app.js",
  "scripts": {
    "start": "node_modules/.bin/cross-env NODE_ENV=production node ./build/app.js",
    "boot": "node_modules/.bin/cross-env NODE_ENV=production babel-node ./build/app.js",
    "dev": "node_modules/.bin/cross-env NODE_ENV=development node_modules/.bin/nodemon --exec babel-node src/app.js --watch src",
    "build": "node_modules/.bin/cross-env NODE_ENV=production babel src --out-dir build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.9"
  },
  "babel": {
    "env": {
      "development": {
        "plugins": [
          "transform-async-to-generator"
        ],
        "presets": [
          "env"
        ]
      },
      "production": {
        "plugins": [
          "transform-async-to-generator"
        ],
        "presets": [
          "env"
        ]
      }
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "figlet": "^1.2.1",
    "morgan": "^1.9.1",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "winston": "^3.1.0"
  }
}
